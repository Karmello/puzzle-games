{
  "name": "puzzle-games",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "node scripts/start.js",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test-win": "set CI=true & npm test",
    "test-coverage": "set CI=true & npm test -- --coverage",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "eslint --ext .js --ext .jsx src",
    "flow": "flow",
    "mongo": "mongod",
    "api": "cd ./../puzzle-games-api && npm start"
  },
  "dependencies": {
    "@mars/heroku-js-runtime-env": "^3.0.2",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "promise": "8.0.1",
    "query-string": "^5.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-draggable": "^3.0.5",
    "react-flexbox-grid": "^2.0.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-swipeable-views": "0.12.4",
    "redux": "^4.0.1",
    "redux-axios-middleware": "^4.0.0",
    "redux-form": "^8.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/react": "^3.4.0",
    "autoprefixer": "7.1.2",
    "babel-eslint": "^8.2.6",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-root-import": "^6.1.0",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "^5.14.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jasmine": "^2.9.2",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "flow-bin": "^0.93.0",
    "flow-webpack-plugin": "^1.2.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "^24.1.0",
    "jest-localstorage-mock": "^2.2.0",
    "marked": "^0.3.12",
    "moxios": "^0.4.0",
    "nock": "^9.1.9",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "react-dev-utils": "^4.1.0",
    "react-hot-loader": "^4.3.11",
    "react-motion": "^0.5.2",
    "react-test-renderer": "^16.2.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/js/constants.js",
      "!src/js/store.js",
      "!src/js/components/index.js",
      "!src/js/containers/index.js",
      "!src/js/reducers/index.js",
      "!src/js/flow-types/*.js",
      "!src/**/*.stories.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/setupTests.js",
      "jest-localstorage-mock"
    ],
    "testMatch": [
      "<rootDir>/src/js/**/*.spec.js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "roots": [
      "./src"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "babel-plugin-root-import"
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
